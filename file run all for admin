UPDATE NHANVIEN SET 
MANQL = 'NV1062' WHERE MANV =  'NV0062';

UPDATE NHANVIEN SET 
MANQL = 'NV3354' WHERE MANV =  'NV0238';

UPDATE NHANVIEN SET 
MANQL = 'NV9606' WHERE MANV =  'NV0243';

UPDATE NHANVIEN SET 
MANQL = 'NV5371' WHERE MANV =  'NV0244';

UPDATE NHANVIEN SET 
