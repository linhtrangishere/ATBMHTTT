CREATE OR REPLACE PROCEDURE USP_CREATEUSER (
    p_username IN VARCHAR2,
    p_password IN VARCHAR2
)
AS
BEGIN
    EXECUTE IMMEDIATE 'CREATE USER ' || p_username || ' IDENTIFIED BY ' || p_password;
    EXECUTE IMMEDIATE 'GRANT CREATE SESSION TO ' || p_username;
    EXECUTE IMMEDIATE 'GRANT CONNECT TO ' || p_username;
    EXECUTE IMMEDIATE 'GRANT CREATE TABLE TO ' || p_username;
    EXECUTE IMMEDIATE 'GRANT UNLIMITED TABLESPACE TO ' || p_username;
END;
/
 
-- Xóa user
CREATE OR REPLACE PROCEDURE USP_DROPUSER (p_username IN VARCHAR2)
AS
BEGIN
    EXECUTE IMMEDIATE 'DROP USER ' || p_username;
END;
/

-- Chỉnh sửa user
CREATE OR REPLACE PROCEDURE USP_ALTERUSER 
(P_USERNAME IN VARCHAR2, P_USER_NEW_PASSWORD IN VARCHAR2)
AS
BEGIN
    EXECUTE IMMEDIATE 'ALTER USER ' || P_USERNAME || ' IDENTIFIED BY ' || P_USER_NEW_PASSWORD;
END;
/

-- Tạo role
CREATE OR REPLACE PROCEDURE USP_ADDROLE (p_role_name IN VARCHAR2)
AS
   v_role_name VARCHAR2(30);
BEGIN
    v_role_name := 'RL_' || p_role_name;
    EXECUTE IMMEDIATE 'CREATE ROLE ' || v_role_name;
END;
/

-- Xóa role
CREATE OR REPLACE PROCEDURE USP_DROPROLE (P_ROLENAME IN VARCHAR2)
AS
BEGIN
    EXECUTE IMMEDIATE 'DROP ROLE ' || P_ROLENAME;
END;
/

--Chỉnh sửa role
CREATE OR REPLACE PROCEDURE USP_ALTERROLE
(P_ROLENAME IN VARCHAR2, P_ROLE_NEW_PASSWORD IN VARCHAR2)
AS
BEGIN
    EXECUTE IMMEDIATE 'ALTER ROLE ' || P_ROLENAME || ' IDENTIFIED BY ' || P_ROLE_NEW_PASSWORD;
END;
/

--tao role 
CREATE ROLE RL_NHANVIEN;
/
CREATE ROLE RL_QLTRUCTIEP;
/
CREATE ROLE RL_TRUONGPHONG;
/
CREATE ROLE RL_TAICHINH;
/
CREATE ROLE RL_NHANSU;
/
CREATE ROLE RL_TRUONGDA;
/
-- tao view, vpd ung voi tung CS cua cac role
-- CS1:
CREATE OR REPLACE VIEW UV_NHANVIEN_NHANVIEN AS
    SELECT *
    FROM NHANVIEN
    WHERE MANV = SYS_CONTEXT('USERENV', 'SESSION_USER');
/
CREATE OR REPLACE VIEW UV_NHANVIEN_PHANCONG AS
    SELECT *
    FROM PHANCONG
    WHERE MANV = SYS_CONTEXT('USERENV', 'SESSION_USER');
/
--CS2:
CREATE OR REPLACE VIEW UV_QLTRUCTIEP_NHANVIEN
AS 
SELECT * FROM NHANVIEN WHERE SYS_CONTEXT('USERENV','SESSION_USER') = MANQL OR SYS_CONTEXT('USERENV','SESSION_USER') = MANV; 
/
CREATE OR REPLACE VIEW UV_QLTRUCTIEP_PHANCONG
AS 
SELECT PHANCONG.* FROM PHANCONG JOIN NHANVIEN ON PHANCONG.MANV = NHANVIEN.MANV WHERE SYS_CONTEXT('USERENV','SESSION_USER') = NHANVIEN.MANQL OR SYS_CONTEXT('USERENV','SESSION_USER') = NHANVIEN.MANV; 
/
CREATE OR REPLACE FUNCTION POL_FUNCTION (P_SCHEMA VARCHAR2, P_OBJ VARCHAR2)
RETURN VARCHAR2
AS 
    USER VARCHAR2(100);
BEGIN
    USER:=SYS_CONTEXT('USERENV', 'SESSION_USER');
    RETURN 'MANV = '''||USER||''''; 
END;
/
begin
    DBMS_RLS.ADD_POLICY(
        OBJECT_SCHEMA => 'ADMIN',
        OBJECT_NAME => 'UV_QLTRUCTIEP_NHANVIEN',
        POLICY_NAME => 'POLICY_QLTRUCTIEP',
        POLICY_FUNCTION => 'POL_FUNCTION',
        STATEMENT_TYPES => 'SELECT',
        SEC_RELEVANT_COLS =>'LUONG, PHUCAP',
        SEC_RELEVANT_COLS_OPT => dbms_rls.ALL_ROWS
    );
end;
/
--CS3:
CREATE OR REPLACE VIEW UV_TRUONGPHONG_NHANVIEN AS
    SELECT *
    FROM NHANVIEN
    WHERE PHG = (SELECT NV2.PHG FROM NHANVIEN NV2 WHERE MANV = SYS_CONTEXT('USERENV', 'SESSION_USER'));
/
begin
    DBMS_RLS.ADD_POLICY(
        OBJECT_SCHEMA => 'ADMIN',
        OBJECT_NAME => 'UV_TRUONGPHONG_NHANVIEN',
        POLICY_NAME => 'POLICY_TRUONGPHONG',
        POLICY_FUNCTION => 'POL_FUNCTION',
        STATEMENT_TYPES => 'SELECT',
        SEC_RELEVANT_COLS =>'LUONG, PHUCAP',
        SEC_RELEVANT_COLS_OPT => dbms_rls.ALL_ROWS
    );
end;
/
CREATE OR REPLACE VIEW UV_TRUONGPHONG_PHANCONG AS
    SELECT PC.*
    FROM NHANVIEN NV JOIN PHANCONG PC ON NV.MANV = PC.MANV
    WHERE PHG IN (SELECT PHG FROM NHANVIEN WHERE MANV = SYS_CONTEXT('USERENV', 'SESSION_USER'));
/
--CS4:Khong can tao view/vpd vi co the thuc hien tren bang
--CS5:
CREATE OR REPLACE VIEW UV_NHANSU_NHANVIEN AS SELECT * FROM NHANVIEN;
begin
    DBMS_RLS.ADD_POLICY(
        OBJECT_SCHEMA => 'ADMIN',
        OBJECT_NAME => 'UV_NHANSU_NHANVIEN',
        POLICY_NAME => 'POLICY_NHANSU',
        POLICY_FUNCTION => 'POL_FUNCTION',
        STATEMENT_TYPES => 'SELECT',
        SEC_RELEVANT_COLS =>'LUONG, PHUCAP',
        SEC_RELEVANT_COLS_OPT => dbms_rls.ALL_ROWS,
        UPDATE_CHECK => TRUE
    );
end;
/
--CS6: 

--RL_NHANVIEN
GRANT SELECT ON PHONGBAN TO RL_NHANVIEN;
GRANT SELECT ON DEAN TO RL_NHANVIEN;
GRANT UPDATE (NGAYSINH,DIACHI,SODT) ON UV_NHANVIEN_NHANVIEN TO RL_NHANVIEN;
GRANT SELECT ON UV_NHANVIEN_NHANVIEN TO RL_NHANVIEN;
GRANT SELECT ON UV_NHANVIEN_PHANCONG TO RL_NHANVIEN;
/
--RL_QLTRUCTIEP
GRANT SELECT ON UV_QLTRUCTIEP_NHANVIEN TO RL_QLTRUCTIEP;
GRANT SELECT ON UV_QLTRUCTIEP_PHANCONG TO RL_QLTRUCTIEP;
/
--RL_NHANSU:
GRANT INSERT, UPDATE ON PHONGBAN TO RL_NHANSU;
GRANT SELECT ON UV_NHANSU_NHANVIEN TO RL_NHANSU;
GRANT UPDATE (MANV,TENNV,PHAI,NGAYSINH,DIACHI,SODT,VAITRO,MANQL,PHG) ON UV_NHANSU_NHANVIEN TO RL_NHANSU;
/
--RL_TRUONGPHONG
GRANT SELECT ON UV_TRUONGPHONG_NHANVIEN TO RL_TRUONGPHONG;
GRANT SELECT, INSERT, DELETE, UPDATE ON UV_TRUONGPHONG_PHANCONG TO RL_TRUONGPHONG;
/
COMMIT;
--RL_TAICHINH
GRANT SELECT ON NHANVIEN TO RL_TAICHINH;
/
GRANT SELECT ON PHANCONG TO RL_TAICHINH;
/
GRANT UPDATE(PHUCAP, LUONG) ON NHANVIEN TO RL_TAICHINH;
/
--RL_TRUONGDA 
GRANT INSERT, UPDATE, DELETE ON DEAN TO RL_TRUONGDA;
/

CREATE OR REPLACE PROCEDURE USP_CREATEUSER
AS
 CURSOR CUR IS (SELECT MANV, VAITRO FROM NHANVIEN WHERE MANV NOT IN (SELECT USERNAME FROM ALL_USERS));
 strSQL VARCHAR(2000);
 ck_User int;
 Usr varchar2(30);
 Usr_role varchar2(30);
 BEGIN
 OPEN CUR;
 LOOP
 FETCH CUR INTO Usr, Usr_role;
 EXIT WHEN CUR%NOTFOUND;
 strSQL := 'SELECT VAITRO FROM NHANVIEN WHERE MANV = '''||Usr||'''';
 EXECUTE IMMEDIATE (strSQL);
 strSQL := 'CREATE USER '||Usr||' IDENTIFIED BY '||Usr;
 EXECUTE IMMEDIATE (strSQL);
 strSQL := 'GRANT CONNECT TO '||Usr;
 EXECUTE IMMEDIATE (strSQL);
 strSQL := 'GRANT CREATE SESSION TO '||Usr;
 EXECUTE IMMEDIATE (strSQL);
 IF Usr_role = N'Nhân viên' THEN 
 EXECUTE IMMEDIATE ('GRANT RL_NHANVIEN TO '||Usr);
 ELSIF Usr_role = N'QL trực tiếp' THEN
 EXECUTE IMMEDIATE ('GRANT RL_QLTRUCTIEP TO '||Usr);
 ELSIF Usr_role = N'Trưởng phòng' THEN
 EXECUTE IMMEDIATE ('GRANT RL_TRUONGPHONG TO '||Usr);
 ELSIF Usr_role = N'Tài chính' THEN
 EXECUTE IMMEDIATE ('GRANT RL_TAICHINH TO '||Usr);
 ELSIF Usr_role = N'Nhân sự' THEN
 EXECUTE IMMEDIATE ('GRANT RL_NHANSU TO '||Usr);
  ELSIF Usr_role = N'Trưởng đề án' THEN
 EXECUTE IMMEDIATE ('GRANT RL_TRUONGDA TO '||Usr);
 END IF;
 END LOOP;
END; 
/
exec USP_CREATEUSER;
/
GRANT RL_NHANVIEN TO RL_NHANSU;
/
GRANT RL_NHANVIEN TO RL_TRUONGDA;
/
GRANT RL_NHANVIEN TO RL_TRUONGPHONG;
/
GRANT RL_NHANVIEN TO RL_QLTRUCTIEP;
/
GRANT RL_NHANVIEN TO RL_TAICHINH;
/
COMMIT;
/


------------------------------------------- NHANVIEN -------------------------------------------
--1. Sửa trên các thuộc tính NGAYSINH, DIACHI, SODT liên quan đến chính nhân viên đó
CREATE OR REPLACE PROCEDURE USP_UPDATE_NHANVIEN_NHANVIEN (
    p_ngaysinh IN DATE,
    p_diachi IN NVARCHAR2,
    p_sodt IN VARCHAR2,
    p_out_message OUT NVARCHAR2
)
AS
    v_manv VARCHAR2(6);
BEGIN
    SELECT MANV INTO v_manv
    FROM UV_NHANVIEN_NHANVIEN;
    
    IF v_manv IS NOT NULL THEN
        UPDATE UV_NHANVIEN_NHANVIEN
        SET
            NGAYSINH = p_ngaysinh,
            DIACHI = p_diachi,
            SODT = p_sodt
        WHERE MANV = v_manv;
        
        IF SQL%ROWCOUNT > 0 THEN
            COMMIT;
            p_out_message := 'Cập nhật thông tin thành công!';
        ELSE
            ROLLBACK;
            p_out_message := 'Không tìm thấy nhân viên!';
        END IF;
    ELSE
        p_out_message := 'Không tìm thấy nhân viên!';
    END IF;
EXCEPTION
    WHEN OTHERS THEN
        ROLLBACK;
        p_out_message := 'Có lỗi xảy ra: ' || SQLERRM;
END;
/
GRANT EXECUTE ON USP_UPDATE_NHANVIEN_NHANVIEN TO RL_NHANVIEN;
/

------------------------------------------- TRUONGPHONG -------------------------------------------
--GRANT EXECUTE ON USP_UPDATE_NHANVIEN_NHANVIEN TO RL_TRUONGPHONG;
--/
--1. Có thể thêm, xóa, cập nhật trên quan hệ PHANCONG liên quan đến các nhân viên thuộc
--phòng ban mà T làm trưởng phòng.

--Thêm phân công
CREATE OR REPLACE PROCEDURE USP_THEM_PHANCONG_TP (
    p_manv IN VARCHAR2,
    p_mada IN VARCHAR2,
    p_ngaybatdau IN DATE,
    p_out_message OUT NVARCHAR2
)
AS
    v_count INTEGER;
BEGIN
    SELECT COUNT(*) INTO v_count
    FROM UV_TRUONGPHONG_PHANCONG
    WHERE MANV = p_manv AND MADA = p_mada;

    IF v_count > 0 THEN
        p_out_message := 'Phân công đã tồn tại';
    ELSE
        INSERT INTO UV_TRUONGPHONG_PHANCONG (MANV, MADA, THOIGIAN)
        VALUES (p_manv, p_mada, p_ngaybatdau);

        IF SQL%ROWCOUNT > 0 THEN
            COMMIT;
            p_out_message := 'Thêm phân công thành công!';
        ELSE
            ROLLBACK;
            p_out_message := 'Có lỗi xảy ra: ' || SQLERRM;
        END IF;
    END IF;
EXCEPTION
    WHEN OTHERS THEN
        ROLLBACK;
        p_out_message := 'Có lỗi xảy ra: ' || SQLERRM;
END;
/
GRANT EXECUTE ON USP_THEM_PHANCONG_TP TO RL_TRUONGPHONG;
/

--Xóa phân công
CREATE OR REPLACE PROCEDURE USP_XOA_PHANCONG_TP (
    p_manv IN VARCHAR2,
    p_mada IN VARCHAR2,
    p_out_message OUT NVARCHAR2
)
AS
    v_count INTEGER;
BEGIN
    SELECT COUNT(*) INTO v_count
    FROM UV_TRUONGPHONG_PHANCONG
    WHERE MANV = p_manv AND MADA = p_mada;

    IF v_count = 0 THEN
        p_out_message := 'Phân công không tồn tại';
    ELSE
        DELETE FROM UV_TRUONGPHONG_PHANCONG
        WHERE MANV = p_manv AND MADA = p_mada;

        IF SQL%ROWCOUNT = 0 THEN
            ROLLBACK;
            p_out_message := 'Có lỗi xảy ra: ' || SQLERRM;
        ELSE
            COMMIT;
            p_out_message := 'Xóa phân công thành công!';
        END IF;
    END IF;
EXCEPTION
    WHEN OTHERS THEN
        ROLLBACK;
        p_out_message := 'Có lỗi xảy ra: ' || SQLERRM;
END;
/
GRANT EXECUTE ON USP_XOA_PHANCONG_TP TO RL_TRUONGPHONG;
/

--Chỉnh sửa phân công
CREATE OR REPLACE PROCEDURE USP_UPDATE_PHANCONG_TP (
    p_manv IN PHANCONG.MANV%TYPE,
    p_mada IN PHANCONG.MADA%TYPE,
    p_thoigian IN PHANCONG.THOIGIAN%TYPE,
    p_out_message OUT NVARCHAR2
) AS
BEGIN
    UPDATE UV_TRUONGPHONG_PHANCONG
    SET THOIGIAN = p_thoigian
    WHERE MANV = p_manv AND MADA = p_mada;

    IF SQL%ROWCOUNT = 0 THEN
        p_out_message := 'Không tìm thấy phân công để cập nhật.';
        ROLLBACK;
    ELSE
        COMMIT;
        p_out_message := 'Cập nhật phân công thành công!';
    END IF;
EXCEPTION
    WHEN OTHERS THEN
        ROLLBACK;
        p_out_message := 'Có lỗi xảy ra: ' || SQLERRM;
END;
/
GRANT EXECUTE ON USP_UPDATE_PHANCONG_TP TO RL_TRUONGPHONG;
/

------------------------------------------- NHANSU -------------------------------------------
--1. Thêm, sửa phòng ban
--Thêm phòng ban
CREATE OR REPLACE PROCEDURE USP_THEM_PHONGBAN_NS (
    p_mapb IN VARCHAR2,
    p_tenpb IN VARCHAR2,
    p_trphg IN VARCHAR2,
    p_out_message OUT NVARCHAR2
)
AS
BEGIN
    INSERT INTO PHONGBAN (MAPB, TENPB, TRPHG)
    VALUES (p_mapb, p_tenpb, p_trphg);

    COMMIT;
    p_out_message := 'Thêm phòng ban thành công!';
EXCEPTION
    WHEN DUP_VAL_ON_INDEX THEN
        ROLLBACK;
        p_out_message := 'Mã phòng ban đã tồn tại!';
    WHEN OTHERS THEN
        ROLLBACK;
        p_out_message := 'Có lỗi xảy ra: ' || SQLERRM;
END;
/
GRANT EXECUTE ON USP_THEM_PHONGBAN_NS TO RL_NHANSU;
/
--Sửa phòng ban
CREATE OR REPLACE PROCEDURE USP_CAPNHAT_PHONGBAN_NS (
    p_mapb IN VARCHAR2,
    p_tenpb IN VARCHAR2,
    p_trphg IN VARCHAR2,
    p_out_message OUT NVARCHAR2
)
AS
BEGIN
    UPDATE PHONGBAN
    SET TENPB = p_tenpb,
        TRPHG = p_trphg
    WHERE MAPB = p_mapb;

    IF SQL%ROWCOUNT > 0 THEN
        COMMIT;
        p_out_message := 'Cập nhật phòng ban thành công!';
    ELSE
        ROLLBACK;
        p_out_message := 'Mã phòng ban không tồn tại!';
    END IF;
EXCEPTION
    WHEN OTHERS THEN
        ROLLBACK;
        p_out_message := 'Có lỗi xảy ra: ' || SQLERRM;
END;
/
GRANT EXECUTE ON USP_CAPNHAT_PHONGBAN_NS TO RL_NHANSU;
/

--2. Thêm, chỉnh sửa nhân viên
--Chỉnh sửa nhân viên
CREATE OR REPLACE PROCEDURE USP_CAPNHAT_NHANVIEN_NS (
    p_manv IN NVARCHAR2,
    p_tennv IN NVARCHAR2,
    p_phai IN NVARCHAR2,
    p_ngaysinh IN DATE,
    p_diachi IN NVARCHAR2,
    p_sodt IN NVARCHAR2,
    p_vaitro IN NVARCHAR2,
    p_manql IN NVARCHAR2,
    p_phg IN NVARCHAR2,
    p_out_message OUT NVARCHAR2
) AS
BEGIN
    UPDATE UV_NHANSU_NHANVIEN
    SET TENNV = p_tennv,
        PHAI = p_phai,
        NGAYSINH = p_ngaysinh,
        DIACHI = p_diachi,
        SODT = p_sodt,
        VAITRO = p_vaitro,
        MANQL = p_manql,
        PHG = p_phg
    WHERE MANV = p_manv;

    IF SQL%ROWCOUNT > 0 THEN
        p_out_message := 'Cập nhật thông tin nhân viên thành công!';
        commit;
    ELSE
        p_out_message := 'Không tìm thấy nhân viên để cập nhật!';
        rollback;
    END IF;
EXCEPTION
    WHEN OTHERS THEN
        p_out_message := 'Lỗi: ' || SQLERRM;
        rollback;
END;
/
GRANT EXECUTE ON USP_CAPNHAT_NHANVIEN_NS TO RL_NHANSU;
/

------------------------------------------- TAICHINH -------------------------------------------
--1. Cập nhật LUONG, PHUCAP của NHANVIEN
CREATE OR REPLACE PROCEDURE USP_UPDATE_LUONG_PHUCAP_NHANVIEN (
    p_manv IN NHANVIEN.MANV%TYPE,
    p_luong IN NHANVIEN.LUONG%TYPE,
    p_phucap IN NHANVIEN.PHUCAP%TYPE,
    p_out_message OUT NVARCHAR2
)
AS
BEGIN
    UPDATE NHANVIEN
    SET LUONG = p_luong, PHUCAP = p_phucap
    WHERE MANV = p_manv;

    IF SQL%ROWCOUNT = 0 THEN
        p_out_message := 'Không tìm thấy phân công để cập nhật.';
        ROLLBACK;
    ELSE
        COMMIT;
        p_out_message := 'Cập nhật phân công thành công!';
    END IF;
EXCEPTION
    WHEN OTHERS THEN
        ROLLBACK;
        p_out_message := 'Có lỗi xảy ra: ' || SQLERRM;
END;
/
GRANT EXECUTE ON USP_UPDATE_LUONG_PHUCAP_NHANVIEN TO RL_TAICHINH;
/

------------------------------------------- TRUONGDEAN -------------------------------------------
--1. Thêm, xóa, sửa đề án
--Thêm đề án
CREATE OR REPLACE PROCEDURE USP_THEM_DEAN (
    p_mada IN VARCHAR2,
    p_tenda IN NVARCHAR2,
    p_ngaybatdau IN DATE,
    p_phong IN VARCHAR2,
    p_out_message OUT NVARCHAR2
)
AS
BEGIN
    INSERT INTO DEAN (MADA, TENDA, NGAYBD, PHONG)
    VALUES (p_mada, p_tenda, p_ngaybatdau, p_phong);

    COMMIT;
    p_out_message := 'Thêm phân công thành công!';
EXCEPTION
    WHEN DUP_VAL_ON_INDEX THEN
        ROLLBACK;
        p_out_message := 'Mã phòng ban đã tồn tại!';
    WHEN OTHERS THEN
        ROLLBACK;
        p_out_message := 'Có lỗi xảy ra: ' || SQLERRM;
END;
/
GRANT EXECUTE ON USP_THEM_DEAN TO RL_TRUONGDA;
/

--Xóa đề án
CREATE OR REPLACE PROCEDURE USP_XOA_DEAN (
    p_mada IN VARCHAR2,
    p_out_message OUT NVARCHAR2
)
AS
    v_count INTEGER;
BEGIN
    SELECT COUNT(*) INTO v_count
    FROM DEAN
    WHERE MADA = p_mada;

    IF v_count = 0 THEN
        p_out_message := 'Phân công không tồn tại';
    ELSE
        DELETE FROM DEAN
        WHERE MADA = p_mada;

        IF SQL%ROWCOUNT = 0 THEN
            ROLLBACK;
            p_out_message := 'Có lỗi xảy ra: ' || SQLERRM;
        ELSE
            COMMIT;
            p_out_message := 'Xóa phân công thành công!';
        END IF;
    END IF;
EXCEPTION
    WHEN OTHERS THEN
        ROLLBACK;
        p_out_message := 'Có lỗi xảy ra: ' || SQLERRM;
END;
/
GRANT EXECUTE ON USP_XOA_DEAN TO RL_TRUONGDA;
/

--Chỉnh sửa đề án
CREATE OR REPLACE PROCEDURE USP_UPDATE_DEAN (
    p_mada IN DEAN.MADA%TYPE,
    p_tenda IN DEAN.TENDA%TYPE,
    p_ngaybatdau IN DEAN.NGAYBD%TYPE,
    p_phong IN DEAN.PHONG%TYPE,
    p_out_message OUT NVARCHAR2
)
AS
BEGIN
    UPDATE DEAN
    SET NGAYBD = p_ngaybatdau, TENDA = p_tenda, PHONG = p_phong
    WHERE MADA = p_mada;

    IF SQL%ROWCOUNT = 0 THEN
        p_out_message := 'Không tìm thấy phân công để cập nhật.';
        ROLLBACK;
    ELSE
        COMMIT;
        p_out_message := 'Cập nhật phân công thành công!';
    END IF;
EXCEPTION
    WHEN OTHERS THEN
        ROLLBACK;
        p_out_message := 'Có lỗi xảy ra: ' || SQLERRM;
END;
/
GRANT EXECUTE ON USP_UPDATE_DEAN TO RL_TRUONGDA;
/
